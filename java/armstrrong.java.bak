
import java.util.Scanner
 class A
 {
	int num,entr,sum=0,rem;
	void set_data()
	{
		System.out.println("Enter a number: ");
		Scanner sc = new Scanner(System.in)
		num=sc.nextint();
		num =entr;
		
		while(num !=0)
		{
			rem=num%10;
			sum=sum+(rem*rem*rem);
			num=num/10;
		}
	}
		void display()
		{
			if(entr == sum)
				System.out.println(entr + " is an Armstrong number.");
			else
				System.out.println(entr+ " is not an Armstrong number.");
		}
}

class Armstrong1
{
	public static void main(String args[])
	{
		A obj = new A();
		obj.set_data();
		obj.display();
	}
}