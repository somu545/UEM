 class B extends  A
  {
  void cal(){
	  
  System.out.println("Enter width: ");
  w=sc.nextInt();
  }
  }
  
 class C extends B
  
 void display()
 {
     a=w*l;
	 System.out.println("area of Rectangle is:  "+a);
  }
  }
  
  
  class rectangle2
  {
